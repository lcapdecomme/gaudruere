{% extends "GaudruereGiteBundle::layouts/gaudruerebaselayout.html.twig" %}

{% block content %} 

<div class="admin">     

  {% block logocontent %}  <a class="logoadmin" href="{{ path('adminGaudruere') }}"></a>{% endblock %}
  <h2>Modification de la page contact</h2>
    
    {{ tinymce_init() }}
    
    <form action="{{ path('adminArticleContact_update') }}" method="post" {{ form_enctype(edit_form) }}>
        <!-- Les erreurs générales du formulaire. -->
        {{ form_errors(edit_form) }}
    
        <div>
            {{ form_label(edit_form.descriptionContact, "Description") }}
            {{ form_widget(edit_form.descriptionContact) }}               
            {{ form_errors(edit_form.descriptionContact) }}
        </div>
        
        <p>
        	<button type="submit">Valider</button>
        </p>    
    </form>    
    
    <a class="retour" href="{{ path('adminGaudruere') }}">Retour à la page d'administration</a>   
</div>

              
{% endblock %}